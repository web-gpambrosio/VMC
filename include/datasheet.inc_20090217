				$disablebuttons = "disabled=\"disabled\"";
			}

	
	$qrydisembreason = mysql_query("SELECT DISEMBREASONCODE,REASON FROM disembarkreason WHERE STATUS=1 AND DISEMBREASONCODE<>'OTH'") or die(mysql_error());
	
