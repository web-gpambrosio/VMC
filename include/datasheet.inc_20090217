													LEFT JOIN manning m ON m.MANNINGCODE=ce.MANNINGCODE
													LEFT JOIN disembarkreason dr ON dr.DISEMBREASONCODE=ce.DISEMBREASONCODE
													WHERE ce.APPLICANTNO=$applicantno
												) x
												ORDER BY x.DATEDISEMB DESC
												LIMIT 1
											") or die(mysql_error());
				
				
			}
			else 
